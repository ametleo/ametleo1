# include <iostream>
using namespace std;

int main()
{
	int s;
	cout <<"Enter Your Exam Score"<<endl;
	cin>> s; 
	//s represents score
	if ( (s<=100) && (s>=80) )
	  cout<<"Grade A"; 
	
	else if ( (s<=79) && (s>=70) )
	    cout <<"Grade B";
	    
	else if ( (s<=69) && (s>=60) )
	    cout <<"Grade C";
	    
	else if ( (s<=59) && (s>=50) )
	    cout <<"Grade D";
	    
	else if ( (s<=49) && (s>=30) )
	    cout <<"Grade E";
	    
	else 
	    cout<<"Grade F";
	    
	return 0;
}
